// "extends": "./.svelte-kit/tsconfig.json"
{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"$libs": ["src/libs"],
			"$libs/*": ["src/libs/*"]
		}
	},
	"include": ["src/**/*.d.ts", "src/**/*.js", "src/**/*.svelte"],
	"exclude": ["node_modules", "**/node_modules", "dist"]
}
